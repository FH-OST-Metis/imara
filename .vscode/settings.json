{
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "markdown-pdf.displayHeaderFooter": true,
  "markdown-pdf.headerTemplate": "<div style=\"font-size: 9pt; margin-left: 1cm;\"><span class=\"title\"></span></div>",
  "markdown-pdf.footerTemplate": "<div style=\"font-size: 9pt; margin-left: 1cm;\"><span class=\"pageNumber\"></span>/<span class=\"totalPages\"></span></div>",
  "markdown-pdf.margin.top": "1cm",
  "markdown-pdf.margin.bottom": "1cm",
  "markdown-pdf.margin.left": "1cm",
  "markdown-pdf.margin.right": "1cm",
  "markdown-pdf.format": "A4",
  "markdown-pdf.quality": 100,
  "markdown-pdf.scale": 1,
  "markdown-pdf.outputDirectory": "docs/9. Projektabschluss/Dokumentation",
  "markdown-pdf.convertOnSave": true,
  "markdown-pdf.convertOnSaveExclude": [
    "README.md"
  ]
}